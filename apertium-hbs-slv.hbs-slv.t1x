	    <test>
	      <equal>
		<clip pos="2" side="sl" part="gen"/>
		<lit-tag v="m"/>
	      </equal>
	    </test>	
	    <let><clip pos="2" side="tl" part="gen"/><lit-tag v="mfn"/></let>
