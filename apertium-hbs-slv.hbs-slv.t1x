	</when>
	<when>
	  <test>
	    <or>
	      <equal><clip pos="1" side="tl" part="tns"/><lit-tag v="aor"/></equal><!-- perf -->
	      <equal><clip pos="1" side="tl" part="tns"/><lit-tag v="pii"/></equal><!-- imperf -->
	    </or>
	  </test>          
	  <let>
	    <clip pos="1" side="tl" part="whole"/>
	    <concat>	 
	      <clip pos="1" side="tl" part="lemh"/>
	      <clip pos="1" side="tl" part="a_vrb"/>
	      <clip pos="1" side="tl" part="asp"/>
	      <clip pos="1" side="tl" part="trn"/>
	      <lit-tag v="past.m"/>
