			<clip pos="1" side="tl" part="cas"/>
			<lit-tag v="ind"/>
		      </lu>
		    </chunk>
		  </out>
		</otherwise>
	      </choose>
	    </otherwise>
	  </choose>
	</otherwise>
      </choose>
    </def-macro>


    <def-macro n="chunk_vbserBiti_adapter" npar="1">
