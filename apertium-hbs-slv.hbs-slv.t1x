	      <clip pos="3" side="tl" part="lemh"/>
	      <clip pos="3" side="tl" part="a_vrb"/>
	      <clip pos="3" side="tl" part="asp"/>
	      <clip pos="3" side="tl" part="trn"/>
	      <lit-tag v="past.m"/>
	      <clip pos="3" side="tl" part="nbr"/>
	    </lu>
	  </chunk>
	</out>
      </action>
